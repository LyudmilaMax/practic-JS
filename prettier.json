{
  "arrowParens": "avoid",
  "bracketSpacing": true,
  "htmlWhitespaceSensitivity": "strict",
  "insertPragma": false,
  "jsxBracketSameLine": false,
  "jsxSingleQuote": false,
"quoteProps": "as-needed",
  "requirePragma": false,
  "semi": true,
  "semicolon": false,
"tabWidth": 2,
  "trailingComma": "all",
"printWidth": 100,
  "useTabs": false,
  "singleQuote": true,
"proseWrap": "always"
}
